{
  "name": "harvest4clients",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8182",
  "scripts": {
    "server": "node server/dist/server.main",
    "start": "concurrently \"nodemon --exec babel-node server/lib/server.main\" \"craco start\"",
    "build": "concurrently \"craco build\" \"babel server/lib -d server/dist --delete-dir-on-start --copy-files\"",
    "test": "craco test",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "greenlock-express": "^2.7.18",
    "greenlock-store-fs": "^3.0.2",
    "public-ip": "^3.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@craco/craco": "^5.5.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "concurrently": "^5.0.0",
    "craco-alias": "^1.0.4",
    "nodemon": "^1.19.4",
    "react-scripts": "^3.2.0"
  }
}
