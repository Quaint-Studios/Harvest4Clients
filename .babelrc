/* .babelrc configuration for the ./server/lib directory */
{
  "presets": ["@babel/preset-env"],
  "plugins": [
    ["module-resolver", {
      "root": ["./server/lib"],
      "alias": {
        "@root": "./",
        "@express": "./server/lib/express",
        "@utils": "./server/lib/utils"
      }
    }]
  ],
  "ignore": []
}